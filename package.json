{
  "name": "bkcli",
  "description": "Helpful personal CLI",
  "version": "0.0.0",
  "author": "Brent Keller @brentkeller",
  "bin": {
    "bk": "./bin/run"
  },
  "bugs": "https://github.com/brentkeller/bkcli/issues",
  "dependencies": {
    "@oclif/core": "^1.13.0",
    "@oclif/plugin-help": "^5.1.12",
    "axios": "^1.6.0",
    "clipboardy": "^2.3.0",
    "date-fns": "^2.21.3",
    "fs-extra": "^10.0.0",
    "node-notifier": "^10.0.0",
    "sound-play": "^1.0.5",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^18.6.3",
    "@types/node-notifier": "^8.0.0",
    "eslint": "^8.2.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11.0.1",
    "oclif": "^3.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/brentkeller/bkcli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "additionalHelpFlags": [
      "-h"
    ],
    "commands": "./lib/commands",
    "bin": "bk",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "brentkeller/bkcli",
  "scripts": {
    "build": "rimraf lib && tsc -b",
    "dev": "rimraf lib && tsc -w",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "yarn build && yarn updatereadme",
    "postpack": "rimraf oclif.manifest.json",
    "test": "echo NO TESTS",
    "updatereadme": "oclif manifest && oclif readme --no-aliases"
  },
  "types": "lib/index.d.ts"
}
